@model ISAT.Business.TicketSolutionList
<div class="row">
    <div class="col-lg-6">
        <div class="form-group">
            <div class="form-group">
                <label>Solution *</label>
                <textarea id="txtArea" rows="10" cols="150"></textarea>
            </div>
        </div>
        <div class="form-group">
            <div class="form-group">
                <button id="addSolution" class="btn btn-sm btn-primary" type="button"><strong>Add Solution</strong></button>
            </div>
        </div>
    </div>
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <table id="tabelMaster" class="table table-striped table-bordered table-hover dataTables-example">
                    <thead>
                        <tr> 
                            <th id="Solution">Solution</th>
                            <th>Time</th>
                            <th>UpdatedBy</th>
                        </tr>
                    </thead>
                    <tbody>
                        @for (int i = 0; i < Model.Solutions.Count; i++)
                        {
                            <tr> 
                                <td>
                                    @Html.HiddenFor(x => x.Solutions[i].HowToSolving)
                                    @Html.DisplayFor(x => x.Solutions[i].HowToSolving)
                                </td>
                                <td>
                                    @Html.HiddenFor(x => x.Solutions[i].UpdatedDate)
                                    @Html.DisplayFor(x => x.Solutions[i].UpdatedDate) 
                                </td>
                                <td>
                                    @Html.HiddenFor(x => x.Solutions[i].UpdatedBy)
                                    @Html.DisplayFor(x => x.Solutions[i].UpdatedBy) 
                                </td>
                            </tr> 
                        } 
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<br />

<table id="tableListClone" style="display: none;">
    <tbody>
        <tr> 
            <td>
                <input type="hidden" id="cloneHowToSolving" />
                <label id="clonelabelHowToSolving"></label> 
            </td>
            <td> 
                <input type="hidden" id="cloneUpdatedDate" />
                <label id="clonelabelUpdatedDate"></label>  
            </td>
            <td>
                <input type="hidden" id="cloneUpdatedBy" />
                <label id="clonelabelUpdatedBy"></label>  
            </td>
        </tr> 
    </tbody>
</table>